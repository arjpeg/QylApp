{
  "expo": {
    "name": "Qyl",
    "slug": "Qyl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Q.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Q.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.arjpeg.Qyl"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
		"plugins": [
			[
				"expo-font",
				{
					"fonts": ["./assets/K2D-Regular.ttf"]
				}
			]
		]
	}
}
